type TokenIndex = nat32;
type AccountIdentifier = text;
type Account = 
 service {
   add: (AccountIdentifier, TokenIndex) -> ();
   addAllowed: (principal) -> ();
   list: (AccountIdentifier) -> (vec TokenIndex);
   rem: (AccountIdentifier, TokenIndex) -> ();
   setup: (record {slot: nat32;}) -> ();
 };
service : () -> Account
